<h1>New singer</h1>

<% form_for(@singer) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%#= f.label :user %><br />
    <%#= f.text_field :user %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', singers_path %>